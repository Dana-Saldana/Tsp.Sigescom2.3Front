<div id="modal-confirmacion-venta-corporativa" class="modal fade" data-keyboard="false" role="dialog" data-backdrop-limit="1" aria-labelledby="Information" data-backdrop="static" aria-hidden="true" data-focus-on="input:first">
    <div class="modal-dialog modal-md">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">CONFIRMAR VENTA CORPORATIVA</h4>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-12">
                        <p>¿ ESTA SEGURO QUE DESEA CONFIRMAR LA VENTA ?</p>
                    </div>
                    <div class="col-md-12" ng-show="mostrarIngresoTrazaDePago()">
                        <div class="box box-primary">
                            <div class="box-body">
                                <div class="row">
                                    <div class="col-md-12">
                                        <label for="mediodepago">MEDIO DE PAGO</label>
                                        <select class="select2 form-control" ng-model="trazaDeInicial.MedioDePago" ng-options="item as item.Nombre for item in mediosDePago track by item.Id"></select>
                                    </div>
                                    <div class="col-md-12">0
                                        <label for="banco">BANCO</label>
                                        <select class="select2 form-control" ng-model="trazaDeInicial.EntidadFinanciera" ng-options="item as item.Nombre for item in entidadesFinancieras track by item.Id"></select>
                                    </div>
                                    <div class="col-md-12">
                                        <label for="informacion">INFORMACIÓN</label>
                                        <textarea class="form-control" rows="3" ng-model="trazaDeInicial.InformacionDePago" placeholder="N° Operación" required></textarea>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button ng-click="guardarConfirmacionVenta()" ng-disabled="datosRequeridosParaRealizarConfimacionVenta()" class="btn btn-primary btn-md"><span class="glyphicon glyphicon-check"></span> ACEPTAR</button>
                <button data-dismiss="modal" title="Cancelar" class="btn btn-md btn-danger"><span class="glyphicon glyphicon-remove"></span> CANCELAR</button>
            </div>
        </div>
    </div>
</div>
