<div id="modal-ver-venta-corporativa" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" data-backdrop="static">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header bg-primary">
                <div class="col-md-12">
                    <h4 class="modal-title">DETALLE DE VENTA CORPORATIVA</h4>
                </div>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-12">
                        <button ng-show="verVentaCorporativa.AccionEditar" title="EDITAR VENTA" ng-click="editarRegistro(verCompraCorporativa.IdOrden)" data-toggle="modal" data-target="#modal-registro-compra-corporativa" class="btn btn-md btn-primary" >EDITAR</button>
                        <button ng-show="verVentaCorporativa.AccionConfirmar" title="CONFIRMAR VENTA" ng-click="iniciarConfirmacion()" data-toggle="modal" data-target="#modal-confirmacion-compra-corporativa" class="btn btn-md btn-primary">CONFIRMAR</button>
                        <button ng-show="verVentaCorporativa.AccionInvalidar" title="ANULAR INTERNAMENTE" ng-init="EsAnuladoConNotaInterna=false" ng-click="limpiarObservacion()" data-toggle="modal" data-target="#modal-anulacion-interna" class="btn btn-md btn-primary" >INVALIDAR</button>
                        <button ng-show="verVentaCorporativa.AccionAnular" title="ANULAR CON NOTA DE CREDITO" ng-init="EsAnuladoConNotaDeCredito=false" ng-click="cargarDatosParaAnulacion()" data-toggle="modal" data-target="#modal-anulacion-nota-credito" class="btn btn-smd btn-primary">ANULAR</button>
                        <button ng-show="verVentaCorporativa.AccionModificarComprobante" title="MODIFICAR COMPROBANTE" ng-click="obtenerTiposDeComprobanteRegistroVenta()" data-toggle="modal" data-target="#modal-registro-comprobante-compra-corporativa" class="btn btn-md btn-primary">COMPROBANTE</button>
                    </div>
                    <div class="col-md-12" style="padding-top:15px">
                        <div class="box box-primary">
                            <div class="box-body">
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="box">
                                            <div class="box-body">
                                                <div class="row">
                                                    <label class="col-md-12">{{verVentaCorporativa.EtiquetaDocumentoIdentidad}} : {{verVentaCorporativa.DocumentoIdentidadProveedor}}</label>
                                                    <label class="col-md-12">RAZON SOCIAL : {{verVentaCorporativa.Proveedor}}</label>
                                                    <label class="col-md-12">{{verVentaCorporativa.DireccionProveedor}}</label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="box">
                                            <div class="box-body">
                                                <div class="row">
                                                    <label class="col-md-12">{{verVentaCorporativa.EtiquetaNombreDocumento}}</label>
                                                    <label class="col-md-12">{{verVentaCorporativa.SerieNumeroDocumento}}</label>
                                                    <label class="col-md-12">Fecha : {{verVentaCorporativa.Fecha}}</label>

                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-12">
                                        <div class="box">
                                            <div class="box-body">
                                                <div class="row">
                                                    <table id="tabla-ver-venta" class="table table-stripped table-bordered">
                                                        <thead>
                                                            <tr>
                                                                <th>Id</th>
                                                                <th>Concepto</th>
                                                                <th>Cantidad</th>
                                                                <th>Precio</th>
                                                                <th>Igv</th>
                                                                <th>Descuento</th>
                                                                <th>Importe</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="item in verVentaCorporativa.Detalles" ng-cloak>
                                                                <td>{{$index+1}}</td>
                                                                <td>{{item.Concepto}}</td>
                                                                <td>{{item.Cantidad|number:numeroDecimalesEnCantidad}}</td>
                                                                <td class="money">S/. {{item.Precio|number:numeroDecimalesEnPrecio}}</td>
                                                                <td class="money">S/. {{item.Igv|number:2}}</td>
                                                                <td class="money">S/. {{item.Descuento|number:2}}</td>
                                                                <td class="money">S/. {{item.Importe|number:2}}</td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                    <div class="col-md-12">
                                                        <div class="col-md-5">
                                                            <label class="col-md-4">Flete : </label>
                                                            <label class="col-md-6">{{verVentaCorporativa.Flete|number:2}}</label>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-5 col-md-offset-7">
                                                        <label class="col-md-6">Sub Total : </label>
                                                        <label class="col-md-6">{{verVentaCorporativa.SubTotal|number:2}}</label>
                                                    </div>
                                                    <div class="col-md-5 col-md-offset-7">
                                                        <label class="col-md-6">Total Igv : </label>
                                                        <label class="col-md-6">{{verVentaCorporativa.Igv|number:2}}</label>
                                                    </div>
                                                    <div class="col-md-5 col-md-offset-7">
                                                        <label class="col-md-6">Importe Total : </label>
                                                        <label class="col-md-6">{{verVentaCorporativa.Total|number:2}}</label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button data-dismiss="modal" ng-click="cerrarVerVenta()" title="CANCELAR" class="btn btn-md btn-danger"><span class="glyphicon glyphicon-remove"></span> CERRAR</button>
            </div>
        </div>
    </div>
</div>

