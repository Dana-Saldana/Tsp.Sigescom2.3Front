<div id="modal-dar-de-baja-sucursal" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel"
     aria-hidden="true" data-backdrop="static">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <a class="close" data-dismiss="modal">&times;</a>
                <h4 class="modal-title">DAR DE BAJA SUCURSAL</h4>
            </div>
            <div class="modal-body">
                <p>Dará de baja a {{nombreSucursal}}</p>
            </div>
            <div class="modal-footer">
                <a class="btn btn-info" data-dismiss="modal" ng-click="eliminarSucursal()">Si</a>
                <a class="btn btn-danger"  data-dismiss="modal">No</a>
            </div>
        </div>
    </div>
</div>

<a id="dar-de-baja-sucursal" data-toggle="modal" data-target="#modal-dar-de-baja-sucursal" style="display:none"></a>