            $('input.number').attr('placeholder', '0.00');
            $('.form-horizontal').on('click', 'input.number', function (data) {
                $(this).attr('placeholder', '0.00');
                if ($(this).val() == '0.00' || $(this).val() == '0') {
                    $(this).val('');
                }
            });
            $(".datepicker").datepicker({
                format: 'dd/mm/yyyy',
                autoclose: true,
                todayHighlight: true,
                language: 'es'
            });
            $(".datepicker-yyyy").datepicker({
                format: 'dd/mm/yyyy',
                autoclose: true,
                todayHighlight: true,
                language: 'es'
            });
            $(".datetimepicker").datetimepicker({
                format: 'LT',
                language: 'es'
            });

            //$(".select22").select2({
            //});
            $(".datepicker-start").datepicker({
                format: 'dd/mm/yyyy',
                autoclose: true,
                todayHighlight: true,
                language: 'es'
            }).on("changeDate", function (data) {
                var date = $(".datepicker-start").datepicker('getDate');
                $(".datepicker-end").datepicker('setStartDate', date);
            }).on('clearDate', function () {
                $('.datepicker-start').datepicker('setDate', null);
            });

            $(".datepicker-end").datepicker({
                format: 'dd/mm/yyyy',
                autoclose: true,
                todayHighlight: true,
                language: 'es'
            }).on("changeDate", function (data) {
                var date = $(".datepicker-end").datepicker('getDate');
                $(".datepicker-start").datepicker('setEndDate', date);
            }).on('clearDate', function () {
                $('.datepicker-end').datepicker('setDate', null);
            });
