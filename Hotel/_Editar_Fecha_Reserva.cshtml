<div id="modal-editar-fecha-reserva" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" data-backdrop="static">
    <div class="modal-dialog modal-md">
        <div class="modal-content">
            <form name="formularioEditarfechaDeReserva" id="formularioEditarfechaDeReserva">
                <div class="modal-header bg-primary">
                    <h4 class="modal-title">EDITAR FECHA DE RESERVA</h4>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-sm-12">
                            <div class="row">
                                <div class="col-sm-4">
                                    <label>Fecha de reserva: </label>
                                </div>
                                <div class="col-sm-6">
                                    <p>({{atencionEditarFecha.FechaIngresoString}}&nbsp;&nbsp;-&nbsp;&nbsp;{{atencionEditarFecha.FechaSalidaString}})</p>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-12">
                            <div class="row">
                                <div class="col-sm-6 form-group">
                                    <label>Desde</label>
                                    <input ng-model="atencionEditarFecha.FechaIngreso" id="fechaIngreso" ng-disabled="atencionEditarFecha.EstadoActual.Id == idEstadoCheckedIn" placeholder="dd/mm/aaaa" class="form-control datepicker-yyyy" data-date-start-date="0d"/>
                                </div>
                                <div class="col-sm-6 form-group">
                                    <label>Hasta</label>
                                    <input ng-model="atencionEditarFecha.FechaSalida" id="fechaSalida" placeholder="dd/mm/aaaa" class="form-control datepicker-yyyy" data-date-start-date="0d"/>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <a title="Guardar" ng-click="guardarFechaAtencion()" ng-disabled="!atencionEditarFecha.FechaIngreso || !atencionEditarFecha.FechaSalida || diferenciaDiasEntreFechas(atencionEditarFecha.FechaIngreso,atencionEditarFecha.FechaSalida) <= 0" class="btn btn-flat btn-primary pull-left"><span class="glyphicon glyphicon-floppy-disk"></span> GUARDAR</a>
                    <a title="Cerrar" ng-click="cerrarFechaAtencion()" class="btn btn-danger btn-flat"><span class="glyphicon glyphicon-remove"></span> CERRAR</a>
                </div>
            </form>
        </div>
    </div>
</div>
